			getGeneProducts, "setGeneProducts<-", 					#Pathway
			getGeneProduct, "setGeneProduct<-", 
			getSets, "setSets<-",														#GeneProductData
			getData, "setData<-", 													#GeneProductData, DataSet
			getIds, "setIds<-", getSystems, "setSystems<-", #GeneProduct
			getFileName, "setFileName<-", hasGeneProduct, 	#Pathway
			getPathways, "setPathways<-",										#PathwaySet
			"==", print, as.matrix													#Generic/primitive
		   )
			
export(DataSet, GeneProduct, GeneProductData, Pathway, PathwaySet)
